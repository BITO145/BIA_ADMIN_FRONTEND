# .gitignore for a React project

# Dependency directories
/node_modules/

.env

# Build output
/dist/
/build/

# React Native (if applicable)
/ios/
/android/


# Next.js (if applicable)
/.next/
/out/

# Yarn lock file (if using yarn)
yarn.lock

# npm lock file
package-lock.json

# dotenv environment variables file
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Next.js/CRA build directories
/.next/
/build/
/out/

# Temporary folders created by IDEs or text editors
/.vscode/
/.idea/
/*.sublime-workspace
/*.sublime-project

# macOS specific files
.DS_Store

# Windows specific files
Thumbs.db

# Log files
*.log

# Optional eslint and prettier configuration
/.eslintcache
/.prettiercache
